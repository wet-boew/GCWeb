<ul class="list-group menu list-unstyled">
<li><h3>{{#each page.localnavHeading}}<a href="{{link}}">{{title}}</a>{{/each}}</h3>
    <ul class="list-group menu list-unstyled">
	{{#each page.localnav}}<li><a href="{{link}}" class="list-group-item">{{title}}</a>
		{{#if subnav}}
	    	<ul class="list-group menu list-unstyled">
				{{#each subnav}}<li><a href="{{link}}" class="list-group-item">{{title}}</a></li>{{/each}}
	        </ul>
		{{/if}}
	</li>{{/each}}
    </ul>
</li>
</ul>
