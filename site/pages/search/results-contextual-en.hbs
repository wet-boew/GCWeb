{
	"title": "Search results (Contextual)",
	"language": "en",
	"altLangPrefix": "results-contextual",
	"pageType": "search",
	"pageclass": "page-type-search",
	"breadcrumb": [
		{ "title": "Home", "link": "https://www.canada.ca/en.html" }
