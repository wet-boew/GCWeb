  "wf:task": [
    {
      "@type": "wf:Task",
      "dct:title": "Compatibility research on how AT are supporting the maxlength attribute",
      "wf:goalDescription": "Document various AT and browser compatibility tests regarding how they are clearing announcing the input content limitation set through the attribute 'maxlength' for a textaria element. Goal is to propose, if issues are found, a solution that could improve the user experience to ensure they are advised when they have reached or close to reach that max length limit.",
      "wf:successCriteria": "A published report with a research summary, any relevant details and related finding.",
      "dct:created": "2023-12-15",
      "acr:requirement": "WCAG21:status-messages",
      "earl:test": [ "WCAG21:status-messages" ],
      "acr:severity": "acr:notRelevant",
      "acr:relevancy": "wbv:acr#comments"
    },
    {
      "@type": "wf:Task",
      "dct:title": "Improve WET-BOEW Postback plugin",
      "wf:goalDescription": "The WET-BOEW Postback plugin should be improved so that the confirmation and error messages are announced by all popular screen-readers. The current solution only displays the message which is not announced by JAWS or NVDA because no text change happened. An authoring tool compatibility research should be conducted to ensure the solution is compatible with the most popular authoring tools.",
      "dct:created": "2025-06-11",
      "acr:requirement": "WCAG21:status-messages",
      "earl:test": [ "WCAG21:status-messages" ],
      "acr:severity": "acr:moderate",
      "acr:relevancy": "wbv:acr#usability"
    },
    {
      "@type": "wf:Task",
      "dct:title": "Add focus to text box on click of No button",
      "wf:goalDescription": "Complete an accessibility research to determine whether or not the text box should be focused when the user clicks the No button. If so, implement the solution.",
      "dct:created": "2025-06-11",
      "acr:requirement": "WCAG21:focus-order",
      "earl:test": [ "WCAG21:focus-order" ],
      "acr:severity": "acr:trivial",
      "acr:relevancy": "wbv:acr#usability"
    },
    {
      "@type": "wf:Task",
      "dct:title": "Enhance redacted text screen-reader alternative",
      "wf:goalDescription": "Perform an accessibility research to determine whether the current text alternative is sufficient or if it should be enhanced. Solution might be to adjust the text to \"personal information redacted\" instead of just \"redacted\".",
      "dct:created": "2025-06-11",
      "acr:requirement": "WCAG21:non-text-content",
      "earl:test": [ "WCAG21:non-text-content" ],
      "acr:severity": "acr:trivial",
      "acr:relevancy": "wbv:acr#usability"
    }
  ],

